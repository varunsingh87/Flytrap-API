{
    "functions": {
        "api/v1/*.php": {
            "runtime": "vercel-php@0.4.0"
        },
        "api/v1/audio/*.php": {
            "runtime": "vercel-php@0.4.0"
        },
        "api/v1/folder/*.php": {
            "runtime": "vercel-php@0.4.0"
        }
    },
    "routes": [
        {
            "src": "/v1/audio",
            "dest": "/api/v1/audio.php"
        },
        {
            "src": "/v1/folder",
            "dest": "/api/v1/folder.php"
        },
        {
            "src": "/v1/audio/collaboration",
            "dest": "/api/v1/audio/collaboration.php"
        },
        {
            "src": "/v1/folder/collaboration",
            "dest": "/api/v1/folder/collaboration.php"
        }
    ]
}